{
  "name": "michelin",
  "version": "0.1.0",
  "main": "lib/index.js",
  "private": true,
  "dependencies": {
    "@antv/data-set": "^0.8.9",
    "antd": "^3.6.4",
    "axios": "^0.18.0",
    "bizcharts": "^3.1.10",
    "classnames": "^2.2.6",
    "enquire.js": "^2.1.6",
    "history": "^4.7.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "moment": "^2.22.2",
    "path-to-regexp": "^2.2.1",
    "rc-drawer": "^1.5.1",
    "react": "^16.6.3",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.6.3",
    "react-fittext": "^1.0.0",
    "react-redux": "^5.0.7",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "url-pattern": "^1.0.3"
  },
  "scripts": {
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "stylus-chokidar src/",
    "prepare": "npm run build-ts && npm run build-css",
    "watch-css": "npm run build-css && stylus-chokidar -w src/ --include-css",
    "start-ts": "react-app-rewired start --scripts-version react-scripts-ts",
    "start": "npm-run-all -p watch-css start-ts",
    "build": "npm run build-css && react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write --no-bracket-spacing --single-quote --trailing-comma=all --print-width=120",
      "git add"
    ]
  },
  "proxy": "http://localhost:8000",
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/enquire.js": "^2.1.1",
    "@types/history": "^4.6.2",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.12",
    "@types/react-document-title": "^2.0.2",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.3",
    "@types/react-router-redux": "^5.0.17",
    "@types/stylus": "^0.48.29",
    "fork-ts-checker-webpack-plugin": "^0.2.10",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.13.7",
    "react-app-rewire-less": "^2.1.2",
    "react-app-rewired": "^1.5.2",
    "react-scripts-ts": "^3.1.0",
    "redux-devtools-extension": "^2.13.7",
    "source-map-explorer": "^1.5.0",
    "stylus": "^0.54.5",
    "stylus-chokidar": "^0.0.3",
    "ts-import-plugin": "^1.5.4",
    "ts-loader": "^5.3.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  }
}
